{
  "expo": {
    "name": "MovieTV",
    "slug": "movie_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo3.png",
    "scheme": "movieapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
  "image": "./assets/images/logo3.png",
  "resizeMode": "contain",
  "backgroundColor": "#0f0d23"
},


    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo3.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.henriquerocha.movie_app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo3.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo3.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f0d23"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1c8ed960-f2b9-44c3-9da2-7ccae4da8b2a"
      }
    }
  }
}
